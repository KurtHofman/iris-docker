{
    "files.associations": {
        "Dockerfile*": "dockerfile",
        "iris.script": "objectscript"
     },
     "intersystems.servers": {
        "docker-iris": {
          "webServer": {
            "scheme": "http",
            "host": "localhost",
            "port": 9092
          },
          "description": "DOCKER-IRIS",
          "username": "_SYSTEM",
          "password": "SYS"
        }
      },
     "objectscript.conn": {
         "server": "docker-iris",
         "ns": "USER",
         "active": true,
         "links": {
          "Production": "http://localhost:${port}/terminal/?ns=USER",
         }
    },
    "sqltools.connections": [
        {
            "namespace": "USER",
            "connectionMethod": "Server and Port",
            "showSystem": false,
            "previewLimit": 50,
            "server": "localhost",
            "port": 9092,
            "askForPassword": false,
            "driver": "InterSystems IRIS",
            "name": "iris-docker",
            "username": "_SYSTEM",
            "password": "SYS"
        }
    ]
}